numeros=[]
for contador in range(1,6):
    nuevo_numero=int(input("Ingrese un nÃºmero por favor: "))
    numeros.append(nuevo_numero)

print(numeros)

num_mayor=numeros[0]

for numero in numeros:
    if (numero>num_mayor):
        num_mayor=numero

print(f"El numero mayor es: {num_mayor} ")
print(f"El numero mayor es: {max(numeros)}")
print(f"El numero menor es: {min(numeros)}")

numeros.sort(reverse=True)
print(numeros)